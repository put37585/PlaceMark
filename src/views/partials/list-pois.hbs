{{#each pois}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{name}}
    </h2>
    <a href="/category/{{ categoryid }}/poi/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-info-circle"></i>
      </span>
    </a>
    <a href="/category/{{ categoryid }}/deletepoi/{{_id}}" class="button">
      <i class="fas fa-trash"></i>
    </a>
  </div>
{{/each}}
